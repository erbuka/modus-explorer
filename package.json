{
  "name": "cultura-nuova",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng build --watch --base-href=/ --output-path build-dev",
    "start:server": "node ./scripts/dev-server.js",
    "start:dev-server": "node ./scripts/dev-server.js --templates --types",
    "build": "ng build --configuration production --base-href=/ --output-path build-prod",
    "build:ghpages": "ng build --configuration production --output-path docs --base-href /cultura-nuova/ && ncp ./assets ./docs/assets",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.17",
    "@angular/cdk": "~12.2.13",
    "@angular/common": "~12.2.17",
    "@angular/compiler": "~12.2.17",
    "@angular/core": "~12.2.17",
    "@angular/forms": "~12.2.17",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "~12.2.17",
    "@angular/platform-browser-dynamic": "~12.2.17",
    "@angular/router": "~12.2.17",
    "animate.css": "^4.1.1",
    "bootstrap": "^4.6.0",
    "leaflet": "^1.7.1",
    "rxjs": "~6.6.3",
    "three": "^0.123.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.18",
    "@angular/cli": "~12.2.18",
