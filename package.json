{
  "name": "cultura-nuova",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng build --watch --base-href=/ --output-path build-dev",
    "start:server": "node ./scripts/dev-server.js",
    "start:dev-server": "node ./scripts/dev-server.js --templates --types",
    "build": "ng build --prod --base-href=/ --output-path build-prod",
    "build:ghpages": "ng build --prod --output-path docs --base-href /cultura-nuova/ && ncp ./assets ./docs/assets",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
