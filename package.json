{
  "name": "cultura-nuova",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng build --watch --base-href=/ --output-path build-dev",
    "start:server": "node ./scripts/dev-server.mjs",
    "start:dev-server": "node ./scripts/dev-server.mjs --templates --types",
    "build": "ng build --configuration production --base-href=/ --output-path build-prod",
    "build:ghpages": "ng build --configuration production --output-path docs --base-href /cultura-nuova/ && ncp ./assets ./docs/assets",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.12",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.2.12",
    "@angular/compiler": "^14.2.12",
    "@angular/core": "^14.2.12",
    "@angular/forms": "^14.2.12",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^14.2.12",
    "@angular/platform-browser-dynamic": "^14.2.12",
    "@angular/router": "^14.2.12",
    "@types/three": "^0.149.0",
    "animate.css": "^4.1.1",
    "bootstrap": "^4.6.0",
    "hammerjs": "^2.0.8",
    "leaflet": "1.9",
    "rxjs": "~6.6.3",
    "three": "^0.149.0",
    "tslib": "^2.0.0",
    "uuid": "^9.0.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.10",
