    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/leaflet": "^1.7.0",
    "@types/node": "^12.12.29",
    "ajv": "^6.12.6",
    "codelyzer": "^6.0.0",
    "colors": "^1.4.0",
    "express": "^4.17.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ncp": "^2.0.0",
    "node-watch": "^0.6.4",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
